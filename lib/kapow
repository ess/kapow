KAPOW_LIB="/Users/dan/Code/kapow/lib/"
KAPOW_FORMULAE="http://github.com/danryan/kapow_repo/"


do_install() {
	echo "Installing $1..."	
}

do_remove() {
	echo "Removing $1..."
}

do_search() {
	echo "Searching for $1..."
}

require() {
  if [ -f $KAPOW_LIB$1 ] && [ -r $KAPOW_LIB$1 ] 
	then
		echo $1
    source "$KAPOW_LIB$1"
	else
		echo "File could not be read or does not exist." && exit
  fi
}

die() {
  echo -e "kapow error:  $1" 1>&2
  exit 255
}